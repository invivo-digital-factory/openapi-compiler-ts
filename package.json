{
  "name": "openapi-compiler-ts",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "author": "InVivo Digital Factory team",
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "lerna bootstrap --no-ci",
    "bootstrap-production": "lerna bootstrap -- --production",
    "release": "lerna publish",
    "test": "lerna run test",
    "test:modules": "lerna run test --scope='@invivodf/module-*'",
    "build": "lerna run build",
    "build:services": "lerna run build --scope='@invivodf/service-*' --include-dependencies",
    "build:modules": "lerna run build --scope='@invivodf/module-*' --include-dependencies",
    "build:interfaces": "lerna run build --scope='@invivodf/module-cart-interfaces'",
    "clean": "lerna run --parallel clean",
    "serve": "lerna run serve --stream",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "typecheck": "lerna run typecheck --stream",
    "documentation:build": "lerna --scope='@invivodf/service-documentation-api' run build",
    "documentation:test": "lerna --scope='@invivodf/service-documentation-api' run test:ci --stream",
    "documentation:serve": "lerna --scope='@invivodf/service-documentation-api' run serve --stream",
    "publish:pre": "lerna publish --canary --preid qua --yes --conventional-prerelease",
    "publish:prod": "lerna publish --yes --conventional-commits"
  },
  "keywords": [
    "example"
  ],
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@types/convict-format-with-validator": "6.0.2",
    "@types/hapi__hapi": "20.0.13",
    "@types/hapi__vision": "5.5.4",
    "@types/mustache": "4.2.2",
    "@types/jest": "^29.2.3",
    "@types/js-yaml": "4.0.5",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mocha": "10.0.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "18.16.16",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "chai": "4.3.7",
    "chai-exclude": "2.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-security": "1.7.1",
    "jest": "^29.3.1",
    "mocha": "10.2.0",
    "mocha-junit-reporter": "2.2.0",
    "mocha-multi-reporters": "1.5.1",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "openapi-types": "12.1.3",
    "prettier": "2.8.8",
    "sinon": "15.2.0",
    "sinon-chai": "3.7.0",
    "supertest": "6.3.3",
    "ts-essentials": "9.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "ts-sinon": "2.0.2",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "husky": "8.0.3",
    "lerna": "6.6.2"
  }
}
